<queue-entries>
  <queue>
    <name>default</name>
    <rate>1/s</rate>
  </queue>
  <queue>
    <name>call-domain-service</name>
    <rate>1/m</rate>
    <retry-parameters>
    	<task-retry-limit>3</task-retry-limit>
    	<min-backoff-seconds>10</min-backoff-seconds>
    </retry-parameters>
  </queue>
  <queue>
    <name>call-workflow</name>
    <rate>1/m</rate>
    <retry-parameters>
    	<task-retry-limit>3</task-retry-limit>
    	<min-backoff-seconds>10</min-backoff-seconds>
    </retry-parameters>
  </queue>
</queue-entries>